				</columns>

				<rows>
					<row>
						<image
							src="/resources/images/icons8-json-64.png" height="32px">
						</image>
						<a href="/views/solicita_arquivo_server.zul">
							Solicitar Arquivo Servidor
						</a>
					</row>
					<row>
						<image src="/resources/images/key.png"
							height="32px">
						</image>
						<a href="/views/decifra_criptografia.zul">
							Decifrar Criptografia
						</a>
					</row>
					<row>
						<image src="/resources/images/icons8-hash.png"
							height="32px">
						</image>
						<a href="/views/gera_hash_criptografado.zul">
							Gerar Hash Criptografado
						</a>
					</row>
					<row>
						<image src="/resources/images/produto.png"
							height="32px">
						</image>
						<a href="/views/envia_solucao.zul">
							Enviar Solução Servidor
						</a>
					</row>
<!-- 					<row> -->
<!-- 						<image src="/resources/images/add_contato.png" -->
<!-- 							height="32px"> -->
<!-- 						</image> -->
<!-- 						<a href="/views/adiciona_contato.zul"> -->
<!-- 							Adiciona Contato -->
<!-- 						</a> -->
<!-- 					</row> -->
				</rows>

			</grid>
		</west>

		<center self="@insert(conteudo)"></center>

		<south height="100px" hflex="1" sclass="rodape">
			<div align="center">
				<vbox align="center">
					Acelera Dev Codenation ${desktop.webApp.version}
					<a href="https://github.com/NecoDan"
						target="_blank">
						Desenvolvedor Responsável
					</a>
				</vbox>
			</div>
		</south>
	</borderlayout>
</zk>
