	<style src="resources/css/bootstrap.css" />
	<style src="resources/css/bootstrap-theme.css" />
	<borderlayout>
		<north height="100px" sclass="banner">
			<div>
				<hbox align="center">
					<a href="index.zul">
						<image src="/resources/images/codenation.png"
							height="90px">
						</image>
					</a>
